{
    "editor.fontFamily": "'Victor Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontWeight": "600",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.tabSize": 2,
    "editor.fontSize": 15,
    "editor.formatOnType": true,
    "editor.lineNumbers": "on",
    "editor.suggestSelection": "first",
    "window.zoomLevel": 0,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Palenight Operator",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "editor.formatOnPaste": true,
    "workbench.editor.highlightModifiedTabs": true,
    "explorer.sortOrder": "type",
    "explorer.autoReveal": false,
    "files.trimFinalNewlines": true,
    "workbench.startupEditor": "none",
    "keyboard.dispatch": "keyCode",
    "rest-client.enableTelemetry": false,
    "rest-client.previewOption": "exchange",
    "rest-client.requestNameAsResponseTabTitle": true,
    "workbench.editor.tabSizing": "shrink",
    "gitlens.currentLine.enabled": false,
    "rest-client.largeResponseBodySizeLimitInMB": 100,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": false,
    "python.languageServer": "Microsoft",
    "zenMode.hideStatusBar": false,
    "zenMode.silentNotifications": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.fullScreen": false,
    "zenMode.centerLayout": false,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vim.sneak": false,
    "vim.camelCaseMotion.enable": false,
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "purple",
    "vim.leader": "<Space>",
    "vim.smartcase": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.visualstar": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 200,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [ "u", "u" ],
            "after": [ "<Esc>" ]
        },
        {
            "before": [ "j", "j" ],
            "after": [ "<Esc>" ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [ "Y" ],
            "after": [ "y", "$" ]
        },
        {
            "before": [ "<leader>", "e", "b" ],
            "commands": [ "workbench.action.navigateBack" ]
        },
        {
            "before": [ "<leader>", "e", "f" ],
            "commands": [ "workbench.action.navigateForward" ]
        },
        {
            "before": [ "<leader>", "e", "e" ],
            "commands": [ "editor.debug.action.selectionToRepl" ]
        },
        {
            "before": [ "<leader>", "e", "r" ],
            "commands": [ "editor.action.rename" ]
        },
        {
            "before": [ "<leader>", "e", "p" ],
            "commands": [ "editor.action.triggerParameterHints" ]
        },
        {
            "before": [ "<leader>", "e", "c" ],
            "commands": [ "editor.action.showContextMenu" ]
        },
        {
            "before": [ "<leader>", "e", "u" ],
            "commands": [ "editor.action.referenceSearch.trigger" ]
        },
        {
            "before": [ "<leader>", "e", "l" ],
            "commands": [ "workbench.action.openRecent" ]
        },
        {
            "before": [ "<leader>", "e", "=" ],
            "commands": [ "editor.action.format" ]
        },
        {
            "before": [ "<leader>", "f", "f" ],
            "commands": [ "workbench.files.action.showActiveFileInExplorer" ]
        },
        {
            "before": [ "<leader>", "f", "o" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "f", "y" ],
            "commands": [ "copyFilePath" ]
        },
        {
            "before": [ "<leader>", "f", "R" ],
            "commands": [ "renameFile" ]
        },
        {
            "before": [ "<leader>", "f", "o" ],
            "commands": [ "workbench.action.files.openFile" ]
        },
        {
            "before": [ "<leader>", "g", "d" ],
            "commands": [ "editor.action.revealDefinition" ]
        },
        {
            "before": [ "<leader>", "g", "i" ],
            "commands": [ "editor.action.goToImplementation" ]
        },
        {
            "before": [ "<leader>", "g", "r" ],
            "commands": [ "editor.action.goToReferences" ]
        },
        {
            "before": [ "<leader>", "g", "s" ],
            "commands": [ "metals.goto-super-method" ]
        },
        {
            "before": [ "<leader>", "g", "f" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "g", "c" ],
            "commands": [ "workbench.action.showAllSymbols" ]
        },
        {
            "before": [ "<leader>", "r", "r" ],
            "commands": [ "editor.action.rename" ]
        },
        // search
        {
            "before": [ "<leader>", "s", "p" ],
            "commands": [ "workbench.view.search" ]
        },
        {
            "before": [ "<leader>", "s", "e" ],
            "commands": [ "workbench.action.findInFiles" ]
        },
        {
            "before": [ "<leader>", "s", "f" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "s", "c" ],
            "commands": [ "workbench.action.showAllSymbols" ]
        },
        {
            "before": [ "<leader>", "s", "r" ],
            "commands": [ "workbench.action.replaceInFiles" ]
        },
        {
            "before": [ "<leader>", "s", "s" ],
            "commands": [ "workbench.action.showAllSymbols" ]
        },
        {
            "before": [ "<leader>", "m", "w" ],
            "commands": [ "workbench.scm.focus" ]
        },
        {
            "before": [ "<leader>", "t", "l" ],
            "commands": [ "workbench.action.nextEditorInGroup" ]
        },
        {
            "before": [ "<leader>", "t", "h" ],
            "commands": [ "workbench.action.previousEditorInGroup" ]
        },
        {
            "before": [ "<leader>", "t", "o" ],
            "commands": [ ":tabo" ]
        },
        {
            "before": [ "<leader>", "t", "r" ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", "t", "c" ],
            "commands": [ "workbench.action.closeActiveEditor" ]
        },
        {
            "before": [ "<leader>", "t", "y" ],
            "commands": [ "" ]
        },
        {
            "before": [ "<leader>", "t", "m" ],
            "commands": [ "workbench.action.moveEditorToLastGroup" ]
        },
        {
            "before": [ "<leader>", "t", "u" ],
            "commands": [ "workbench.action.reopenClosedEditor" ]
        },
        {
            "before": [ "<leader>", "t", "j" ],
            "commands": [ "workbench.action.joinAllGroups" ]
        },
        {
            "before": [ "<leader>", "v", "p" ],
            "commands": [ "editor.action.triggerParameterHints" ]
        },
        {
            "before": [ "<leader>", "v", "h" ],
            "commands": [ "editor.action.showHover" ]
        },
        {
            "before": [ "<leader>", "v", "i", "c" ],
            "commands": [ "metals.toggle-implicit-conversions-and-classes" ]
        },
        {
            "before": [ "<leader>", "v", "i", "a" ],
            "commands": [ "metals.toggle-implicit-parameters" ]
        },
        {
            "before": [ "<leader>", "v", "i", "t" ],
            "commands": [ "metals.toggle-show-inferred-type" ]
        },
        {
            "before": [ "<leader>", "w", "c" ],
            "commands": [ "workbench.action.closeActiveEditor" ]
        },
        {
            "before": [ "<leader>", "w", "o" ],
            "commands": [ "workbench.files.action.closeOtherFiles" ]
        },
        {
            "before": [ "<leader>", "w", "r" ],
            "commands": [ "workbench.action.files.revealActiveFileInWindows" ]
        },
        {
            "before": [ "<leader>", "w", "=" ],
            "commands": [ "workbench.action.evenEditorWidths" ]
        },
        {
            "before": [ "<leader>", "w", "h" ],
            "commands": [ "workbench.action.focusLeftGroup" ]
        },
        {
            "before": [ "<leader>", "w", "l" ],
            "commands": [ "workbench.action.focusRightGroup" ]
        },
        {
            "before": [ "<leader>", "y", "t" ],
            "commands": [ "" ]
        },
        {
            "before": [ "<leader>", "y", "p" ],
            "commands": [ "copyRelativeFilePath" ]
        },
        {
            "before": [ "<leader>", "<tab>" ],
            "commands": [ "workbench.action.openPreviousEditor" ]
        },
        {
            "before": [ "u" ],
            "commands": [ "undo" ]
        },
        {
            "before": [ "<C-r>" ],
            "commands": [ "redo" ]
        },
        {
            "before": [ "<Return>" ],
            "commands": [ ":nohl" ]
        },
        {
            "before": [ "Z", "Z" ],
            "commands": [ ":wq" ]
        },
        // smart move between windows
        {
            "before": ["<C-k>"],
            "commands": ["workbench.action.navigateUp"],
        },
        {
            "before": ["<C-j>"],
            "commands": ["workbench.action.navigateDown"],
        },
        {
            "before": ["<C-h>"],
            "commands": ["workbench.action.navigateLeft"],
        },
        {
            "before": ["<C-l>"],
            "commands": ["workbench.action.navigateRight"],
        },
     ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ "<leader>", "e", "=" ],
            "commands": [ "editor.action.format" ]
        },
        {
            "before": [ ">" ],
            "commands": [ "editor.action.indentLines" ]
        },
        {
            "before": [ "<" ],
            "commands": [ "editor.action.outdentLines" ]
        },
        {
            "before": [ "<leader>", "e", "e" ],
            "commands": [ "editor.debug.action.selectionToRepl" ]
        },
    ],
    "editor.defaultFormatter": "scalameta.metals",
    "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToImplementation",
    "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
    "editor.tabCompletion": "on",
    "workbench.enableExperiments": false,
    "workbench.tips.enabled": false,
    "workbench.editor.showTabs": false,
    "breadcrumbs.enabled": true,
    "vim.foldfix": true,
    "metals.javaHome": "/usr/lib/jvm/java-8-openjdk"
}