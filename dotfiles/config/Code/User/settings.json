    "editor.tabSize": 2,
    "editor.fontSize": 15,
    "editor.formatOnType": true,
    "editor.lineNumbers": "on",
    "editor.suggestSelection": "first",
    "cSpell.language": "en,es",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "editor.formatOnPaste": true,
    "workbench.editor.highlightModifiedTabs": true,
    "explorer.sortOrder": "type",
    "explorer.autoReveal": false,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.startupEditor": "none",
    "keyboard.dispatch": "keyCode",
    "rest-client.enableTelemetry": false,
    "rest-client.previewOption": "exchange",
    "rest-client.requestNameAsResponseTabTitle": true,
    "workbench.editor.tabSizing": "shrink",
    "gitlens.currentLine.enabled": false,
    "rest-client.largeResponseBodySizeLimitInMB": 100,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": false,
